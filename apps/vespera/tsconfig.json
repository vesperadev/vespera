{
  "extends": ["@vercel/style-guide/typescript"],
  "compilerOptions": {
    "target": "es2022",
    "module": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "moduleResolution": "NodeNext",
    "experimentalDecorators": true
  }
}
